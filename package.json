{
  "name": "@tager/admin-services",
  "version": "1.0.0-rc.27",
  "description": "Tager admin services",
  "repository": "git@github.com:ozitag/tager-admin-services.git",
  "author": "Ivan Saranchonkau <ivan.saranchenkov@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build-js": "vite build",
    "copy-dts": "copyfiles -f custom-types/* dist",
    "emit-declarations": "tsc --project tsconfig.json --emitDeclarationOnly && yarn copy-dts",
    "build": "concurrently --kill-others-on-fail true \"yarn emit-declarations\" \"yarn run build-js\"",
    "prepare": "yarn build"
  },
  "dependencies": {
    "i18next": "^21.6.15",
    "nanoid": "^3.3.2",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@types/node": "16.x",
    "concurrently": "^7.1.0",
    "copyfiles": "^2.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vue": "3.2.31",
    "vue-router": "4.0.14"
  },
  "peerDependencies": {
    "vue": ">=3.2.x",
    "vue-router": ">=4.x"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  }
}
